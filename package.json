        "@svgr/webpack": "^8.1.0",
        "@symfony/stimulus-bridge": "^3.0.0",
        "@symfony/webpack-encore": "^5.1.0",
        "@types/react-dom": "^19.1.2",
        "@types/react": "^19.1.2",
        "autoprefixer": "^10.4.20",
        "ckeditor5": "^45.0.0",
        "core-js": "^3.0.0",
        "eslint": "^8.3.0",
