{
    "browserslist": {
        "defaults": [
            "last 2 versions",
            "Safari >= 10",
            "iOS >= 10",
            "not ie <= 10",
            "> 1%"
        ],
        "evergreen": [
            "last 2 Chrome versions",
            "last 2 ChromeAndroid versions",
            "last 2 Firefox versions",
            "last 2 FirefoxAndroid versions",
            "last 2 Safari versions",
            "last 2 iOS versions",
            "last 2 Edge versions",
            "last 2 Opera versions"
        ]
    },
    "scripts": {
        "ibexa:dev": "encore dev --config ./node_modules/@ibexa/frontend-config/ibexa.webpack.${configName:-all}.configs",
        "ibexa:watch": "encore dev --watch --config ./node_modules/@ibexa/frontend-config/ibexa.webpack.${configName:-all}.configs",
        "ibexa:build": "encore production --progress --config ./node_modules/@ibexa/frontend-config/ibexa.webpack.${configName:-all}.configs",
        "build:dev": "encore dev --config=webpack.config.project.js",
        "build:prod": "encore production --config=webpack.config.project.js",
        "watch": "encore dev --watch --config=webpack.config.project.js",
        "start": "encore dev-server --config=webpack.config.project.js",
        "server": "encore dev-server --config=webpack.config.project.js",
        "site:dev": "encore dev --config-name --config=webpack.config.project.js",
        "site:prod": "encore production --config-name --config=webpack.config.project.js",
        "site:watch": "encore dev --watch --config-name --config=webpack.config.project.js",
        "site:server": "encore dev-server --config-name --config=webpack.config.project.js",
        "format:js": "prettier --write \"assets/js/**/*.js\"",
        "linter:js": "eslint \"assets/js/**/*.js\""
    },
    "private": true,
    "devDependencies": {
        "@babel/core": "^7.4.3",
        "@babel/preset-env": "^7.4.3",
