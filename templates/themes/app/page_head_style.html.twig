<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&display=swap" rel="stylesheet">

{% for href in encore_entry_css_files('app', 'app') %}
    <link rel="preload" as="style" type="text/css" href="{{ href }}" onload="this.onload=null;this.rel='stylesheet';"></link>
{% endfor %}
{% for href in encore_entry_css_files('app-noncore', 'app') %}
    <link rel="prefetch" as="style" type="text/css" href="{{ href }}" onload="this.onload=null;this.rel='stylesheet';"></link>
{% endfor %}

<link rel="preload" as="style" type="text/css" href="{{ asset('style.css', 'nglayouts_' ~ (nglayouts.debug ? 'dev_' : '') ~ 'css') }}" onload="this.onload=null;this.rel='stylesheet';"/>
<link rel="preload" as="style" type="text/css" href="{{ asset('style.css', 'nglayouts_standard_' ~ (nglayouts.debug ? 'dev_' : '') ~ 'css') }}" onload="this.onload=null;this.rel='stylesheet';"/>
