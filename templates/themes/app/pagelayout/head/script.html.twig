{% for src in encore_entry_js_files('index', 'app') %}
    {% if 'vendor' in src %}
        <script defer type="text/javascript" src="{{ src }}"></script>
    {% endif %}
{% endfor %}

{{ vite_entry_script_tags('index') }}
{{ vite_entry_script_tags('index-noncritical', { attr: { "defer": true } }) }}


<script defer type="text/javascript" src="{{ asset('app.js', 'nglayouts_' ~ (nglayouts.debug ? 'dev_' : '') ~ 'js') }}"></script>

{{ nglayouts_template_plugin('preview.javascripts') }}
